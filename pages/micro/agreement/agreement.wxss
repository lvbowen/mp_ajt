
.page-agreement{
  background: #fff;
  font-size:28rpx;
  color: #333;
  padding:30rpx;
  margin-top:64px;
}
.ag-title {
  text-align: center;
}
.section{
  margin-top:20rpx;
  margin-bottom: 10rpx;
}
.section .hd{
  font-weight: bold;
  font-size: 30rpx;
}
.section .hd-inner{
  font-weight: bold;
}
.section .li{
  position:relative;
  padding-left:20rpx;
} 
.section .li::before{
  content: '';
  position: absolute;
  top:16rpx;
  left:0;
  width:8rpx;
  height: 8rpx;
  border-radius: 50%;
  background: #000;
}